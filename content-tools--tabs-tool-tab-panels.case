<!--# TEST ID: CT05-->
<!--Try and add a tab-->
<tr>
	<td>click</td>
	<td>//a[@id=&quot;ees_screenMenucontentPageStandard&quot;]</td>
	<td></td>
</tr>
<tr>
	<td>waitForVisible</td>
	<td>css=img[alt=&quot;Loading&quot;]</td>
	<td></td>
</tr>
<tr>
	<td>waitForNotVisible</td>
	<td>css=img[alt=&quot;Loading&quot;]</td>
	<td></td>
</tr>
<tr>
	<td>storeCssCount</td>
	<td>css=.ui-tabs-title</td>
	<td>originalNumberOfTabs</td>
</tr>
<tr>
	<td>click</td>
	<td>//div[@data-folder-id=&quot;${newTabsAssetID}&quot;]/div/a[@title=&quot;Add a new tab&quot;]</td>
	<td></td>
</tr>
<tr>
	<td>waitForVisible</td>
	<td>id=confirmButton</td>
	<td></td>
</tr>
<tr>
	<td>click</td>
	<td>id=confirmButton</td>
	<td></td>
</tr>
<tr>
	<td>waitForVisible</td>
	<td>css=img[alt=&quot;Loading&quot;]</td>
	<td></td>
</tr>
<tr>
	<td>waitForNotVisible</td>
	<td>css=img[alt=&quot;Loading&quot;]</td>
	<td></td>
</tr>
<tr>
	<td>waitForVisible</td>
	<td>css=.ui-tabs-title</td>
	<td></td>
</tr>
<tr>
	<td>storeCssCount</td>
	<td>css=.ui-tabs-title</td>
	<td>currentNumberOfTabs</td>
</tr>
<tr>
	<td>verifyEval</td>
	<td>${originalNumberOfTabs} + 1</td>
	<td>${currentNumberOfTabs}</td>
</tr>
<tr>
	<td>storeAttribute</td>
	<td>//div[@data-folder-id=&quot;${newTabsAssetID}&quot;]/div/div/ul/li[last()]@data-tab-nav-id</td>
	<td>newTabAssetID</td>
</tr>
<!---->
<!--Reposition the tab-->
<tr>
	<td>click</td>
	<td>//li[@data-tab-nav-id=&quot;${newTabAssetID}&quot;]/a[@class=&quot;bodycopy_re_order icon&quot;]</td>
	<td></td>
</tr>
<tr>
	<td>waitForVisible</td>
	<td>//li[contains(@class,&quot;child-${newTabAssetID}&quot;)]/a</td>
	<td></td>
</tr>
<tr>
	<td>dragAndDropToObject</td>
	<td>//li[contains(@class,&quot;child-${newTabAssetID}&quot;)]/a</td>
	<td>//li[contains(@class,&quot;child-${newTabAssetID}&quot;)]/parent::ul/li[1]</td>
</tr>
<tr>
	<td>click</td>
	<td>id=ees_reorderOverlay_save</td>
	<td></td>
</tr>
<tr>
	<td>waitForVisible</td>
	<td>css=img[alt=&quot;Loading&quot;]</td>
	<td></td>
</tr>
<tr>
	<td>waitForNotVisible</td>
	<td>css=img[alt=&quot;Loading&quot;]</td>
	<td></td>
</tr>
<tr>
	<td>verifyAttribute</td>
	<td>//div[@data-folder-id=&quot;${newTabsAssetID}&quot;]/div/div/ul/li[1]@data-tab-nav-id</td>
	<td>${newTabAssetID}</td>
</tr>
<!---->
<!--Rename the tab-->
<tr>
	<td>click</td>
	<td>//li[@data-tab-nav-id=&quot;${newTabAssetID}&quot;]/a[@title=&quot;Edit Container Properties&quot;]</td>
	<td></td>
</tr>
<tr>
	<td>waitForVisible</td>
	<td>id=${newTabAssetID}</td>
	<td></td>
</tr>
<tr>
	<td>type</td>
	<td>id=${newTabAssetID}</td>
	<td>New Tab</td>
</tr>
<tr>
	<td>click</td>
	<td>id=edit_properties_ok_${newTabAssetID}</td>
	<td></td>
</tr>
<tr>
	<td>waitForVisible</td>
	<td>css=img[alt=&quot;Loading&quot;]</td>
	<td></td>
</tr>
<tr>
	<td>waitForNotVisible</td>
	<td>css=img[alt=&quot;Loading&quot;]</td>
	<td></td>
</tr>
<tr>
	<td>verifyText</td>
	<td>//a[@href=&quot;#tabs-${newTabAssetID}&quot;]</td>
	<td>New Tab</td>
</tr>
<!---->
<!--Delete the tab-->
<tr>
	<td>waitForVisible</td>
	<td>//li[@data-tab-nav-id=&quot;${newTabAssetID}&quot;]/a[@class=&quot;bodycopy_delete icon&quot;]</td>
	<td></td>
</tr>
<tr>
	<td>click</td>
	<td>//li[@data-tab-nav-id=&quot;${newTabAssetID}&quot;]/a[@class=&quot;bodycopy_delete icon&quot;]</td>
	<td></td>
</tr>
<tr>
	<td>waitForVisible</td>
	<td>id=confirmButton</td>
	<td></td>
</tr>
<tr>
	<td>click</td>
	<td>id=confirmButton</td>
	<td></td>
</tr>
<tr>
	<td>waitForVisible</td>
	<td>css=img[alt=&quot;Loading&quot;]</td>
	<td></td>
</tr>
<tr>
	<td>waitForNotVisible</td>
	<td>css=img[alt=&quot;Loading&quot;]</td>
	<td></td>
</tr>
<tr>
	<td>verifyElementNotPresent</td>
	<td>//li[contains(@class,&quot;child-${newTabAssetID}&quot;)]</td>
	<td></td>
</tr>
<!---->
